 <div class="well">
 	  
 	
	                    	<div class="header">User Table </div>
	                    	
	                    	<div class="table-responsive">
	                    		<table class="table table-hover">
	                                <thead>
	                                    <tr>
	                                        <th>Email</th>
	                                        <th>Date registered</th>
	                                        <th>Name</th>
	                                        
	                                        <th></th>
	                                    </tr>
	                                </thead>
	                                <tbody>
	                                	<% @brand.organization_users.each do |user| %>
		                                    <tr>
		                                        <td><%= user.email %> </td>
		                                        <td><%= user.created_at.strftime("%Y/%m/%d") %></td>
		                                        <td><%= user.name.try(:humanize) %></td>                                                  
		                                        <td>
		                                        	<%= link_to "Show", organization_organization_user_path(@brand,user) %>
		                                        </td>                              
		                                    </tr>
	                                    <% end %>
	                                    
	                                </tbody>
	                            </table>
	                    	</div>
	                    	</div>