<div class="well cust_form">
  	<div >
	    <div class="form-group cust_show_form">
	      <h3>Account Information</h3>
	      <div style="width: 20%">
 	 		<%= link_to "Edit Account", edit_organization_organization_account_path(@organization, @account), class: "dbtn big twitter-big no-image btn-block"  %>	</div>
	      <p class="text-muted ">
	      <h4>Name</h4> : <h4><span><%=@account.name%></span></h4>
	      </p>
	      <h4>General Note</h4> : <h4><span><%=@account.general_note%></span></h4>
	      </p>
	  	</div>
	  	<div class="form-group cust_show_form">
	      <h4>Contact Informations</h4>
	      <p class="text-muted ">
	      <%=render "contact_table"%>
	      </p>
	  	</div>
	  	<div class="form-group cust_show_form">
	      <h4>Adresses</h4>
	      <p class="text-muted ">
	      <%=render "address_table"%>
	      </p>
	  	</div>
	  	<div class="form-group cust_show_form">
	      <h4>Brand Status</h4>
	      <p class="text-muted ">
	      <%=render "brand_table"%>
	      </p>
	  	</div>
	</div>
</div>

